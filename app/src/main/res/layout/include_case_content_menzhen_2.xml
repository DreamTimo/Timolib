<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:id="@+id/case_root_2"
    android:visibility="gone"
    android:orientation="vertical">
    <TextView
        android:id="@+id/tv_copy_choice"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/image_25"
        android:layout_marginRight="@dimen/image_25"
        android:layout_marginTop="10dp"
        android:text="复印内容"
        android:textColor="@color/text_black"
        android:textSize="@dimen/type_12sp" />
    <LinearLayout
        android:layout_marginTop="10dp"
        android:background="@color/white"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/image_25"
            android:layout_marginRight="@dimen/image_25"
            android:layout_marginTop="20dp"
            android:text="门诊"
            android:drawablePadding="5dp"
            android:drawableLeft="@mipmap/case_open"
            android:textColor="@color/text_black"
            android:textSize="@dimen/type_12sp" />
        <LinearLayout
            android:layout_marginTop="10dp"
            android:orientation="vertical"
            android:layout_marginLeft="@dimen/img_10"
            android:layout_marginRight="@dimen/img_10"
            android:background="@drawable/shape_case_content"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginTop="10dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <RadioButton
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:textSize="@dimen/img_13"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/img_14"
                    android:button="@null"
                    android:drawableLeft="@drawable/selector_check_box"
                    android:drawablePadding="10dp"
                    android:text="门诊就诊记录"
                    android:textColor="@color/black_333"
                    tools:ignore="RtlHardcoded" />
                <RadioButton
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:textSize="@dimen/img_13"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/img_14"
                    android:button="@null"
                    android:drawableLeft="@drawable/selector_check_box"
                    android:drawablePadding="10dp"
                    android:text="住院病例（入院记录）"
                    android:textColor="@color/black_333"
                    tools:ignore="RtlHardcoded" />
            </LinearLayout>

            <View
                style="@style/line"
                android:layout_marginTop="@dimen/dp_10" />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginTop="10dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">
                <RadioButton
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/img_14"
                    android:button="@null"
                    android:drawableLeft="@drawable/selector_check_box"
                    android:drawablePadding="10dp"
                    android:text="门（急）诊病历首页"
                    android:textSize="@dimen/img_13"
                    android:textColor="@color/black_333"
                    tools:ignore="RtlHardcoded" />
                <RadioButton
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:textSize="@dimen/img_13"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/img_14"
                    android:button="@null"
                    android:drawableLeft="@drawable/selector_check_box"
                    android:drawablePadding="10dp"
                    android:text="化验单（检查报告）"
                    android:textColor="@color/black_333"
                    tools:ignore="RtlHardcoded" />
            </LinearLayout>

            <View
                style="@style/line"
                android:layout_marginTop="@dimen/dp_10" />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginTop="10dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">
                <RadioButton
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:textSize="@dimen/img_13"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/img_14"
                    android:button="@null"
                    android:drawableLeft="@drawable/selector_check_box"
                    android:drawablePadding="10dp"
                    android:text="医学影像学检查资料"
                    android:textColor="@color/black_333"
                    tools:ignore="RtlHardcoded" />
                <RadioButton
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/img_14"
                    android:button="@null"
                    android:drawableLeft="@drawable/selector_check_box"
                    android:drawablePadding="10dp"
                    android:text="病理细胞学"
                    android:textColor="@color/black_333"
                    tools:ignore="RtlHardcoded" />
            </LinearLayout>

            <View
                style="@style/line"
                android:layout_marginTop="@dimen/dp_10" />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginTop="10dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">
                <RadioButton
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:textSize="@dimen/img_13"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/img_14"
                    android:button="@null"
                    android:drawableLeft="@drawable/selector_check_box"
                    android:drawablePadding="10dp"
                    android:text="腔镜"
                    android:textColor="@color/black_333"
                    tools:ignore="RtlHardcoded" />
                <RadioButton
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/img_14"
                    android:button="@null"
                    android:drawableLeft="@drawable/selector_check_box"
                    android:drawablePadding="10dp"
                    android:text="其他"
                    android:textColor="@color/black_333"
                    tools:ignore="RtlHardcoded" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>